<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhanglin.practice"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:icon="@drawable/fifth"
        android:label="5th Player" >
        <activity
            android:name=".MainActivity"
            android:label="5th Player" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SongsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".PlayingActivity" >
        </activity>
        <activity android:name=".ArtistActivity" >
        </activity>
        <activity android:name=".art" >
        </activity>
        <activity android:name=".AlbumActivity" >
        </activity>
        <activity android:name=".alb" >
        </activity>
        <activity android:name=".PlayListActivity" >
        </activity>

        <service android:name=".PlayerServices" />

        <activity
            android:name=".About"
            android:label="关于Fifth Player"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
    </application>

</manifest>